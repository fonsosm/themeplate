  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": true,
      "customGradient": true,
      "defaultGradients": false,
      "defaultPalette": false,
      "defaultDuotone": false,
      "palette": [
        {
          "color": "oklch(94% 0 0)",
          "name": "Background Primary",
          "slug": "background-primary"
        },
        {
          "color": "oklch(71% 0.18 28)",
          "name": "Accent Primary",
          "slug": "accent-primary"
        },
        {
          "color": "oklch(22% 0 0)",
          "name": "Text Primary",
          "slug": "text-primary"
        },
        {
          "color": "oklch(79% 0 0)",
          "name": "Background Secondary",
          "slug": "background-secondary"
        },
        {
          "color": "oklch(37% 0 0)",
          "name": "Text Secondary",
          "slug": "text-secondary"
