        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--16)",
          "lineHeight": "1.5",
          "letterSpacing": "0em"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--inter)",
          "fontWeight": ""
        }
      },
      "link": {}
    },
    "spacing": {},
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--16)",
      "fontStyle": "normal",
      "fontWeight": "400",
      "lineHeight": "1.5",
      "letterSpacing": "0.03em"
    }
