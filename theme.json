        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/inter/Inter-VariableFont_opsz,wght.ttf"
              ]
            },
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/inter/Inter-Italic-VariableFont_opsz,wght.ttf"
              ]
            }
          ],
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "inter"
