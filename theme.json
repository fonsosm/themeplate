{
  "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": true,
      "customGradient": true,
      "defaultGradients": false,
      "defaultPalette": false,
      "defaultDuotone": false,
      "palette": [
        {
          "color": "oklch(94% 0 0)",
          "name": "Background Primary",
          "slug": "background-primary"
        },
        {
          "color": "oklch(71% 0.18 28)",
          "name": "Accent Primary",
          "slug": "accent-primary"
        },
        {
          "color": "oklch(22% 0 0)",
          "name": "Text Primary",
          "slug": "text-primary"
        },
        {
          "color": "oklch(79% 0 0)",
          "name": "Background Secondary",
          "slug": "background-secondary"
        },
        {
          "color": "oklch(37% 0 0)",
          "name": "Text Secondary",
          "slug": "text-secondary"
        },
        {
          "color": "oklch(61% 0.18 28)",
          "name": "Accent Primary Dark",
          "slug": "accent-primary-dark"
        },
        {
          "color": "oklch(84% 0.18 28)",
          "name": "Accent Primary Light",
          "slug": "accent-primary-light"
        }
      ]
    },
    "custom": {
      "line-height": {
        "body": 1.3,
        "heading": 1.06
      }
    },
    "layout": {
      "contentSize": "620px",
      "wideSize": "1440px"
    },
    "spacing": {
      "defaultSpacingSizes": false,

      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "name": "1",
          "size": "1rem",
          "slug": "10"
        },
        {
          "name": "2",
          "size": "min(1.5rem, 2vw)",
          "slug": "20"
        },
        {
          "name": "3",
          "size": "min(2.5rem, 3vw)",
          "slug": "30"
        },
        {
          "name": "4",
          "size": "min(4rem, 5vw)",
          "slug": "40"
        },
        {
          "name": "5",
          "size": "min(6.5rem, 8vw)",
          "slug": "50"
        },
        {
          "name": "6",
          "size": "min(10.5rem, 13vw)",
          "slug": "60"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fluid": true,
      "defaultFontSizes": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/inter/Inter-VariableFont_opsz,wght.ttf"
              ]
            },
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/inter/Inter-Italic-VariableFont_opsz,wght.ttf"
              ]
            }
          ],
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "inter"
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "name": "XS",
          "size": "0.875rem",
          "slug": "fs-xs"
        },
        {
          "fluid": false,
          "name": "S",
          "size": "1rem",
          "slug": "fs-s"
        },
        {
          "fluid": false,
          "name": "M",
          "size": "1.25rem",
          "slug": "fs-m"
        },
        {
          "fluid": {
            "max": "1.5rem",
            "min": "1.25rem"
          },
          "name": "L",
          "size": "1.5rem",
          "slug": "fs-l"
        },
        {
          "fluid": {
            "max": "1.875rem",
            "min": "1.5rem"
          },
          "name": "XL",
          "size": "1.875rem",
          "slug": "fs-xl"
        },
        {
          "fluid": {
            "max": "2.25rem",
            "min": "1.875rem"
          },
          "name": "XXL",
          "size": "2.25rem",
          "slug": "fs-xxl"
        },
        {
          "fluid": {
            "max": "2.8125rem",
            "min": "2.25rem"
          },
          "name": "3XL",
          "size": "2.8125rem",
          "slug": "fs-3xl"
        },
        {
          "fluid": {
            "max": "3.5625rem",
            "min": "2.8125rem"
          },
          "name": "4XL",
          "size": "3.5625rem",
          "slug": "fs-4xl"
        },
        {
          "fluid": {
            "max": "4.5rem",
            "min": "3.5625rem"
          },
          "name": "5XL",
          "size": "4.5rem",
          "slug": "fs-5xl"
        },
        {
          "fluid": {
            "max": "5.625rem",
            "min": "4.5rem"
          },
          "name": "6XL",
          "size": "5.625rem",
          "slug": "fs-6xl"
        }
      ],
      "writingMode": true
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background-primary)",
      "text": "var(--wp--preset--color--text-primary)"
    },
    "elements": {
      "button": {},
      "caption": {},
      "h1": {
        "color": {
          "text": "var(--wp--preset--color--text-text-primary)"
        },
        "typography": {
          "fontSize": "",
          "lineHeight": "",
          "letterSpacing": ""
        }
      },
      "h2": {
        "color": {
          "text": "var(--wp--preset--color--text-text-primary)"
        },
        "typography": {
          "fontSize": "",
          "lineHeight": "",
          "letterSpacing": ""
        }
      },
      "h3": {
        "color": {
          "text": "var(--wp--preset--color--text-text-primary)"
        },
        "typography": {
          "fontSize": "",
          "lineHeight": "",
          "letterSpacing": ""
        }
      },
      "h4": {
        "color": {
          "text": "var(--wp--preset--color--text-text-primary)"
        },
        "typography": {
          "fontSize": "",
          "lineHeight": "",
          "letterSpacing": ""
        }
      },
      "h5": {
        "color": {
          "text": "var(--wp--preset--color--text-text-primary)"
        },
        "typography": {
          "fontSize": "",
          "lineHeight": "",
          "letterSpacing": ""
        }
      },
      "h6": {
        "color": {
          "text": "var(--wp--preset--color--text-text-primary)"
        },
        "typography": {
          "fontSize": "",
          "lineHeight": "",
          "letterSpacing": ""
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--inter)",
          "fontWeight": ""
        }
      },
      "link": {}
    },
    "spacing": {},
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--fs-s)",
      "fontStyle": "normal",
      "fontWeight": "400",
      "lineHeight": "1.55"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ]
}
